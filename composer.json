{
    "name": "harrysbaraini/laravel-auditing-mongo-db",
    "description": "Keep a change history for your models using laravel and lumen with mongo db",
    "keywords": ["audit", "auditing", "activity", "laravel", "log", "logging", "register", "observer", "lumen"],
    "homepage": "",
    "type": "package",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/thialves/laravel-auditing-mongo-db/issues",
        "source": "https://github.com/thialves/laravel-auditing-mongo-db"
    },
    "authors": [
        {
            "name": "Thiago Alves",
            "email": "talves.ads@gmail.com"
        },
        {
            "name": "Vanderlei Sbaraini Amancio",
            "email": "harrysbaraini@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/bus": "5.2.*|5.3.*|5.4.*",
        "illuminate/contracts": "5.2.*|5.3.*|5.4.*",
        "illuminate/database": "5.2.*|5.3.*|5.4.*",
        "illuminate/queue": "5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.2.*|5.3.*|5.4.*",
        "ramsey/uuid": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "ThiAlves\\Auditing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "minimum-stability": "dev"
}
